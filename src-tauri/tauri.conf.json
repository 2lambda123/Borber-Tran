{
  "productName": "Tran",
  "version": "0.2.1",
  "identifier": "com.borber.tran",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5046",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon.icns",
      "icons/128x128@2x.png",
      "icons/128x128.png",
      "icons/32x32.png"
    ]
  }
}